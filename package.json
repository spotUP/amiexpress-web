  "scripts": {
    "dev": "node dev-server.js",
    "dev:frontend": "cd amiexpress-web && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:watch": "nodemon --watch backend/src --watch amiexpress-web/src --ext ts,tsx,js,jsx --delay 2 --exec \"npm run restart-dev\"",
    "restart-dev": "npm run kill-dev && sleep 2 && concurrently --kill-others \"npm run dev:frontend\" \"npm run dev:backend\"",
    "kill-dev": "pkill -f \"vite\" && pkill -f \"node.*backend\" && pkill -f \"ts-node\" && sleep 3",
    "start": "npm run dev",
    "dev:stable": "npm run kill-dev && sleep 2 && concurrently --kill-others \"npm run dev:frontend\" \"npm run dev:backend\"",
    "test": "echo \"Error: no test specified\" && exit 1"
