# Environment variables for AmiExpress Web Backend
PORT=3001
NODE_ENV=production
# PostgreSQL database URL (for Vercel/Render.com deployment)
DATABASE_URL=postgresql://username:password@hostname:5432/database_name
# Alternative for Vercel
POSTGRES_URL=postgresql://username:password@hostname:5432/database_name
JWT_SECRET=your-jwt-secret-here
SESSION_TIMEOUT=3600000
MAX_SESSIONS=100
SYSOP_USERNAME=sysop
SYSOP_PASSWORD=change-this-password